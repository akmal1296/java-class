import java.util.*;
class Main
{
    public static void main(String[] args)
    {
        Scanner scan=new Scanner(System.in);
        System.out.println("Ente the Numbers");
        int a=scan.nextInt();
        int b=scan.nextInt();
        System.out.println("Before Swaping");
        System.out.println(a+" "+b);
        System.out.println("After Swaping");
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println(a+" "+b);
    }
}
